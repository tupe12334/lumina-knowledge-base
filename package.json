{
  "name": "@lumina/knowledge-base",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "pnpm run generate",
    "prelint": "pnpm run generate",
    "build": "pnpm exec nest build",
    "format": "prettier --write \"src/**/*.ts\" \"prisma/**/*.ts\" \"!src/generated/**\" \"!generated/**\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"prisma/**/*.ts\" \"!src/generated/**\" \"!generated/**\"",
    "start": "pnpm exec nest start",
    "dev": "pnpm run start:dev",
    "start:dev": "pnpm exec nest start --watch",
    "start:debug": "pnpm exec nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:e2e": "node dist/main",
    "preview": "pnpm run build && pnpm run start:prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --run --inspect",
    "test:e2e": "vitest run test/**/*.e2e-spec.ts",
    "test:ui": "vitest --ui",
    "test:update": "vitest --run --update",
    "test:seed": "vitest run prisma/seed",
    "test:db": "vitest run prisma/db.snapshot.spec.ts",
    "test:db:update": "vitest run prisma/db.snapshot.spec.ts --update",
    "generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:db:push": "prisma db push --force-reset",
    "seed": "prisma db seed",
    "prisma:studio": "prisma studio",
    "db:drop": "prisma migrate reset --force",
    "db:reset": "prisma migrate reset --force --skip-generate",
    "db:dump": "sqlite3 ./prisma/main.db .dump > dump.sql",
    "predb:dump:apply": "rm -rf ./prisma/main.db",
    "db:dump:apply": "sqlite3 ./prisma/main.db < dump.sql",
    "add-data": "gemini --yolo --model='gemini-2.5-flash' --include-directories ./prisma -p='Please update the seed script and add 3 new math module and 30 questions relevant to each module in various difficulty levels, make sure the new module applies to the existing knowledge base by reading the existing modules, dont forget to update and create the relevant relationships and to make sure the script is working as expected, This is only one prompt conversion so make sure to finish my task in and dont ask for my responses. Use the current implementation of the seed and do not create new files. Make sure to run the update to the snapshot and the 'test:seed' command, and do not forget to maintain the i18n support.'"
  },
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@nestjs/apollo": "^13.1.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/graphql": "^13.1.0",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^7.1.6",
    "@nestjs/terminus": "^11.0.0",
    "@prisma/client": "^6.12.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "graphql": "^16.11.0",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^11.1.0",
    "zod": "^4.0.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.10.7",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "prisma": "^6.12.0",
    "prisma-json-schema-generator": "^5.1.5",
    "prismock": "^1.35.4",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "vitest": "^3.2.4"
  },
  "vitest": {
    "test": {
      "environment": "node",
      "globals": true
    }
  },
  "prisma": {
    "seed": "./prisma/seed/seed.sh"
  }
}
